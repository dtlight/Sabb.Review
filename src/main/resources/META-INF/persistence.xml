<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">

    <persistence-unit name="SabbReview" >
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.sabbreview.model.User</class>
        <class>com.sabbreview.model.Application</class>
        <class>com.sabbreview.model.Assignment</class>
        <class>com.sabbreview.model.Role</class>
        <class>com.sabbreview.model.Comment</class>
        <class>com.sabbreview.model.Field</class>
        <class>com.sabbreview.model.FieldInstance</class>
        <class>com.sabbreview.model.FieldOption</class>
        <class>com.sabbreview.model.FieldType</class>
        <class>com.sabbreview.model.Template</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://127.0.0.1:5432/SabbReview?user=cs2855"/>
            <property name="javax.persistence.jdbc.driver " value="org.postgresql.Driver"/>
            <property name="eclipselink.target-database" value="PostgreSQL" />
            <property name="eclipselink.ddl-generation" value="create-or-extend-tables" />
            <property name="eclipselink.ddl-generation.output-mode" value="database" />
            <property name="eclipselink.query-results-cache" value="false"/>
            <property name="eclipselink.cache.shared.default" value="false"/>

        </properties>
    </persistence-unit>
</persistence>
